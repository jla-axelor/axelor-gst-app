<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
  
	<grid model="com.axelor.gst.db.Sequence" title="Sequences" name="sequence_grid">
		<field name="prefix" />
		<field name="suffix" />
		<field name="nextNumber" />
		<field name="incrementOf" />
		<field name="padding" />
		<field name="model" />
	</grid>
	<form model="com.axelor.gst.db.Sequence" title="Sequence" name="sequence_form" css="panel-pink">
		<panel name="mainPanel" title="Sequence">
			<field name="prefix" />
			<field name="suffix" />
			<field name="nextNumber" />
			<field name="incrementOf" />
			<field name="padding" />
			<field name="model" />			
		</panel>
	</form>

</object-views>