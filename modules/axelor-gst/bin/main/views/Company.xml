<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	
	<grid model="com.axelor.gst.db.Company" title="Companys" name="company_grid">
		<field name="name" />
		<field name="logo" />
		<field name="gstin" />
		<field name="address" />
	</grid>
	
	<form model="com.axelor.gst.db.Company" title="Company" name="company_form">
		<panel name="mainPanel">
			<field name="name" />
			<field name="logo" />
			<panel-related field="contact" name="contact" />
			<field name="gstin" />
			<panel-related field="bankDetails" name="bankDetails" />
			<field name="address" />
		</panel>
	</form>	

</object-views>
