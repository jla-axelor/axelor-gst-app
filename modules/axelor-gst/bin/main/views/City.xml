<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	
	<grid model="com.axelor.gst.db.City" title="Cities" name="city_grid">
		<field name="name" />
		<field name="state" />
		<field name="country" />
	</grid>
	
	<form model="com.axelor.gst.db.City" title="City" name="city_form" css="panel-green">
		<panel name="mainPanel" title="City">
		<field name="name" />
		<field name="state" />
		<field name="country" />
		</panel>
	</form>

</object-views>